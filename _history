lper function to control the motors\nfunction setMotors(leftSpeed: number, rightSpeed: number): void {\n    // Ensure speed is within valid range (-100 to 100)\n    leftSpeed = Math.max(-100, Math.min(100, leftSpeed));\n    rightSpeed = Math.max(-100, Math.min(100, rightSpeed));\n\n    // Determine motor directions\n    const leftDirection = leftSpeed >= 0 ? MotorDirection.Forward : MotorDirection.Reverse;\n    const rightDirection = rightSpeed >= 0 ? MotorDirection.Forward : MotorDirection.Reverse;\n\n    // Set motor speeds and directions\n    motobit.setMotorSpeed(Motor.Left, leftDirection, Math.abs(leftSpeed));\n    motobit.setMotorSpeed(Motor.Right, rightDirection, Math.abs(rightSpeed));\n}\n\n// Function to make the robot drive in a circle\nfunction driveInCircle(times: number): void {\n    for (let i = 0; i < times; i++) {\n        // One motor slower for circular motion\n        setMotors(50, 20);\n        basic.pause(3000); // Adjust duration for a full circle\n    }\n    setMotors(0, 0); // Stop after the circles\n}\n\n// Function to move back and forth\nfunction driveBackAndForth(times: number): void {\n    for (let i = 0; i < times; i++) {\n        // Drive forward\n        setMotors(50, 50);\n        basic.pause(1000); // Move forward for 1 second\n\n        // Drive backward\n        setMotors(-50, -50);\n        basic.pause(1000); // Move backward for 1 second\n    }\n    setMotors(0, 0); // Stop after moving back and forth\n}\n\n// Function to start the program when A+B is pressed\ninput.onButtonPressed(Button.AB, () => {\n    basic.showString(\"GO\"); // Display \"GO\" on the LED screen\n    driveInCircle(2);  // Drive in a circle 2 times\n    driveBackAndForth(2); // Drive back and forth 2 times\n    basic.showString(\"DONE\"); // Indicate completion\n});\n"]],"start1":0,"start2":0,"length1":1758,"length2":0}]}]},{"timestamp":1733770082938,"editorVersion":"7.0.51","changes":[{"type":"edited","filename":"main.blocks","patch":[{"diffs":[[0,"es><"],[-